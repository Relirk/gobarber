    "express-handlebars": "^3.1.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.4",
    "multer": "^1.4.1",
    "nodemailer": "^6.3.0",
    "nodemailer-express-handlebars": "^3.0.0",
    "pg": "^7.11.0",
